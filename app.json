{
  "expo": {
    "name": "Vybe",
    "slug": "Vybe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/Logo_Vybe_512.png",
    "splash": {
      "image": "./assets/images/logos/VYBE_logo_black_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": ["vybe"],
    "platforms": ["ios", "android"],  
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/Logo_Vybe_1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Vybe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logos/V_logo_white.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logos/VYBE_logo_black_transparent.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
